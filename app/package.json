{
   "name": "referral-program-service",
   "version": "0.0.1",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "npm run build && node ./build/src/server/index.js",
      "build": "rm -rf ./build && tsc -p . && npm run swagger && npm run migration:run",
      "swagger": "tsoa swagger",
      "test": "jest --runInBand --forceExit --no-cache",
      "coverage": "jest --coverage",
      "test:watch": "jest --watch",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migration:run": "npx typeorm-ts-node-esm migration:run -d ./build/src/server/appDataSource.js",
      "migration:generate": "npx typeorm-ts-node-esm migration:generate ./src/migrations/'${npm_config_archive}' -d ./src/server/appDataSource.ts",
      "seed:create": "npm run typeorm migration:create -n ./src/migrations/'${npm_config_archive}'"
   },
   "keywords": [],
   "author": "BB Squad",
   "license": "ISC",
   "dependencies": {
      "@telecom-argentina/config": "^1.0.0",
      "@telecom-argentina/logger": "^0.1.8",
      "@telecom-argentina/microservice-middlewares": "^0.2.4",
      "cors": "^2.8.5",
      "crypto-js": "^4.1.1",
      "dayjs": "^1.10.7",
      "dotenv": "^8.2.0",
      "export-from-json": "^1.5.1",
      "express": "^4.17.1",
      "express-http-context": "^1.2.4",
      "http-errors": "^1.8.0",
      "i": "^0.3.7",
      "join": "^3.0.0",
      "json-rules-engine": "^6.1.2",
      "jwt-simple": "^0.5.6",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.3.0",
      "tsoa": "^2.4.11",
      "typeorm": "0.3.6",
      "uuid": "^8.3.2",
      "validatorjs": "^3.22.1"
   },
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/crypto-js": "^4.1.0",
      "@types/express": "^4.17.11",
      "@types/jest": "^26.0.23",
      "@types/jwt-simple": "^0.5.33",
      "@types/node": "^16.11.10",
      "@types/swagger-ui-express": "^4.1.3",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "jest": "^26.6.3",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "ts-jest": "^26.5.4",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}