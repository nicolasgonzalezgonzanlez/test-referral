{
  "swagger":{
    "entryFile": "./src/server/index.ts",
    "outputDirectory": "./build/src",
    "host": "localhost:8083",
    "basePath": "/api/",
    "noImplicitAdditionalProperties": "silently-remove-extras",
    "securityDefinitions": {
      "Auth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "routes":{
    "entryFile": "./src/server/index.ts",
    "routesDir": "./src",
    "basePath": "/api/",
    "authenticationModule": "./src/server/middleware/authRestrict.ts"
  }
}
